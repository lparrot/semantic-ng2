{
  "name": "semantic-ng2",
  "version": "0.0.9",
  "description": "Semantic-UI package for Angular2",
  "main": "semantic-ng2.js",
  "scripts": {
    "tsc": "tsc",
    "start": "node server.js",
    "heroku-postbuild": "npm run app:install",
    "app:install": "gulp clean && npm install && typings install && bower install && tsc && gulp",
    "npm:outdated": "npm outdated",
    "registry:npmjs": "npm set registry https://registry.npmjs.org",
    "registry:sinopia": "npm set registry http://localhost:4873",
    "registry:publish": "tsc && gulp && npm run registry:sinopia && npm publish"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "body-parser": "^1.15.1",
    "bower": "^1.7.9",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "del": "^2.2.0",
    "express": "^4.13.4",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-concat": "2.6.0",
    "gulp-filter": "4.0.0",
    "gulp-minify": "0.0.11",
    "gulp-minify-css": "1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-sass": "2.3.1",
    "gulp-strip-comments": "2.4.3",
    "gulp-strip-css-comments": "1.2.0",
    "jsonwebtoken": "^7.0.0",
    "lodash": "^4.13.1",
    "mongoose": "^4.4.19",
    "morgan": "^1.7.0",
    "typescript": "1.8.10",
    "typings": "0.8.1"
  },
  "author": "PARROT Laurent <laurent.parrot78@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lparrot/semantic-ng2.git"
  },
  "keywords": [
    "semantic",
    "ui",
    "semantic-ui",
    "angular2"
  ],
  "license": "ISC",
  "bugs": "https://github.com/lparrot/semantic-ng2/issues"
}
